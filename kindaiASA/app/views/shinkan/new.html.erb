<h1>新歓情報登録</h1>

<%= link_to "Top",home_path %><br>

<%= form_for(@shinkan, url: {controller: 'shinkan',action: 'new'}) do |f| %>
  <%= f.label :部活名 %>
  <%= f.text_field :club %>
  <%= f.label :日付 %>
  <%= f.date_field :date %>
  <%= f.label :時間 %>
  <%= f.text_field :time %>
  <%= f.label :場所 %>
  <%= f.text_field :place %>
  <%= f.label :コメント %>
  <%= f.text_field :comment %>
  
  <%= f.submit("作成") %>
<% end %>